<ul>
  <% @products.each do |product| %>
    <li><%= link_to(product.title, admin_product_path(product)) %></li>
    <li><%= link_to("edit", edit_admin_product_path(product)) %></li>
    <li><%= link_to("delete", admin_product_path(product), method: :delete, data: {confirm:"是否删除"}) %></li>
  <% end %>
</ul>
